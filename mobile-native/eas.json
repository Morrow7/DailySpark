{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "cache": {
        "key": "android-cache-v1",
        "paths": ["/root/.gradle/caches", "/root/.gradle/wrapper"]
      }
    },
    "production": {
      "autoIncrement": true,
      "cache": {
        "key": "android-cache-v1",
        "paths": ["/root/.gradle/caches", "/root/.gradle/wrapper"]
      }
    }
  },
  "submit": {
    "production": {}
  }
}
